function guessBlue(blueStart, redStart, bluePulled, redPulled) {
  // Your code here
  let blueRemains = blueStart - bluePulled
  insideTotal = (blueStart - bluePulled) + redStart - redPulled
//pull out so far //total
  return blueRemains/insideTotal

}

const TestAsert = (valueA, valueB) => {
    return console.log(
        valueA === valueB ? "no error : " + valueA : "error : " + valueA
    );
};

TestAsert(guessBlue(5, 5, 2, 3), 0.6);
TestAsert(guessBlue(5, 7, 4, 3), 0.2);
TestAsert(guessBlue(12, 18, 4, 6), 0.4);